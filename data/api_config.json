{
  "chat": {
    "name": "OnlySq Chat API",
    "description": "Text conversation using OnlySq models",
    "enabled": true,
    "default": true,
    "api_key": "openai",
    "model": "deepseek-v3",
    "base_url": "https://api.onlysq.ru/ai/openai",
    "endpoint": "/v1/chat/completions",
    "max_tokens": 4096
  },
  "chatvision": {
    "name": "OnlySq Vision API",
    "description": "Chat with image understanding",
    "enabled": true,
    "default": false,
    "api_key": "openai",
    "model": "gpt-4o-mini",
    "base_url": "https://api.onlysq.ru/ai/openai",
    "endpoint": "/v1/chat/completions",
    "max_tokens": 4096,
    "supports_vision": true
  },
  "image": {
    "name": "OnlySq Image API",
    "description": "Generate images via OnlySq's Flux/Schnell",
    "enabled": true,
    "default": false,
    "api_key": "openai",
    "model": "flux-schnell",
    "base_url": "https://api.onlysq.ru/ai/openai",
    "endpoint": "/v1/images/generations",
    "max_tokens": 1000
  }
}